<Application x:Class="View.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:View"
			 xmlns:viewmodel="clr-namespace:ViewModel;assembly=ViewModel">
    <Application.Resources>
		<DataTemplate DataType="{x:Type viewmodel:PlayPuzzleVM}">
			<local:PlayPuzzle />
		</DataTemplate>
		<DataTemplate DataType="{x:Type viewmodel:SaveScoreVM}">
			<local:SaveScore />
		</DataTemplate>
		<DataTemplate DataType="{x:Type viewmodel:IntroductionVM}">
			<local:Introduction />
		</DataTemplate>
		<DataTemplate DataType="{x:Type viewmodel:EditPuzzleVM}">
			<local:EditPuzzle />
		</DataTemplate>
		<DataTemplate DataType="{x:Type viewmodel:ChooseGameVM}">
			<local:ChooseGame />
		</DataTemplate>
	</Application.Resources>
</Application>
